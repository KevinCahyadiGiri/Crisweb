				<input type="text" name="nama" class="form-control" placeholder="Nama Menu">
			</div>
			<div class="form-group mx-sm-3 mb-2">
					<input type="textarea" name="nama" class="form-control" placeholder="Nama Menu">
			</div>
			<button type="submit" value='upload' class="btn btn-outline-warning mb-2" id='button'>Submit</button>
		</form>	
